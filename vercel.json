{
    "builds":[
    {
        "src":"github_visitor_info_admin/wsgi.py",
        "use":"@vercel/python"
    },
    {
        "src":"build.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles"
        }
    }
],
"routes":[
    {
        "src":"/(.*)",
        "dest":"github_visitor_info_admin/wsgi.py"
    },
    {
        "src":"/static/(.*)",
        "dest":"/static/$1"
    }
]
}